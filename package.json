{
  "name": "eleventy-dot-js-blog",
  "version": "0.1.1",
  "description": "A starter project showing how to build a blog with the Eleventy static site generator using JavaScript templates.",
  "main": "index.js",
  "scripts": {
    "build": "ELEVENTY_ENV=production npx eleventy & NODE_ENV=production postcss css/main.css --o _site/css/main.css",
    "dev": "ELEVENTY_ENV=development npx eleventy --serve & postcss css/main.css --o _tmp/main.css --watch",
    "debug": "DEBUG=* npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/reubenlillie/eleventy-dot-js-blog.git",
    "https": "https://gitlab.com/reubenlillie/eleventy-dot-js-blog.git",
    "editUrl": "https://gitlab.com/reubenlillie/eleventy-dot-js-blog/-/edit/master"
  },
  "author": "Reuben L. Lillie",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/reubenlillie/eleventy-dot-js-blog/issues"
  },
  "homepage": "https://gitlab.com/reubenlillie/eleventy-dot-js-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "clean-css": "^4.2.3",
    "html-minifier": "^4.0.0",
    "postcss-cli": "^7.1.1",
    "terser": "^4.6.13"
  },
  "dependencies": {
    "alpinejs": "^2.6.0",
    "autoprefixer": "^9.8.6",
    "css-loader": "^4.2.2",
    "laravel-mix": "^5.0.5",
    "laravel-mix-purgecss": "^5.0.0",
    "mini-css-extract-plugin": "^0.11.0",
    "postcss-clean": "^1.1.0",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^2.3.0",
    "tailwindcss": "^1.7.6",
    "webpack": "^4.44.1"
  }
}
